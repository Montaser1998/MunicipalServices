            <th>
                @Html.DisplayNameFor(model => model.RemainingFeesDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BillOfFees)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BillRemainingFees)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateOfIssuance)
