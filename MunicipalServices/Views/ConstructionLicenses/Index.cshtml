            <th>
                @Html.DisplayNameFor(model => model.Basin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Part)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConstructionUse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConstructionLicenseType)
            </th>
            <th>
