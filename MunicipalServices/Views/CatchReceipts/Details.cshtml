@model MunicipalServices.Data.CatchReceipts

@{
    ViewData["Title"] = "Details";
}


<div class="content-box mt-2">
    <div class="content-box-header">
        <h3 class="content-box__title">Catch Receipts</h3>
    </div>

    <div class="content-box-body">

        <div class="content-box-body__row row">

            <div class="form-item-valid form-group col-12 col-md-6">
                <label class="input-label"> @Html.DisplayNameFor(model => model.FullName)</label>
                <input class="form-control input" id="inputtext-2" disabled="disabled" value="@Html.DisplayFor(model => model.FullName)">
            </div>

            <div class="form-item-valid form-group col-12 col-md-6">
                <label class="input-label">@Html.DisplayNameFor(model => model.AmountOfMoneyNumber)</label>
                <input class="form-control input" id="inputtext-2" disabled="disabled" value="@Html.DisplayFor(model => model.AmountOfMoneyNumber)">
            </div>

            <div class="form-item-valid form-group col-12 col-md-6">
                <label class="input-label"> @Html.DisplayNameFor(model => model.AmountOfMoneyText)</label>
                <input class="form-control input" id="inputtext-2" disabled="disabled" value="@Html.DisplayFor(model => model.AmountOfMoneyText)">
            </div>

            <div class="form-item-valid form-group col-12 col-md-6">
                <label class="input-label"> @Html.DisplayNameFor(model => model.Reason)</label>
                <input class="form-control input" id="inputtext-2" disabled="disabled" value="@Html.DisplayFor(model => model.Reason)">
            </div>
            <div class="form-item-valid form-group col-12 col-md-6">
                <label class="input-label"> @Html.DisplayNameFor(model => model.ToAccount)</label>
                <input class="form-control input" id="inputtext-2" disabled="disabled" value="@Html.DisplayFor(model => model.ToAccount)">
            </div>
            <div class="form-item-valid form-group col-12 col-md-6">
                <label class="input-label"> @Html.DisplayNameFor(model => model.Currency)</label>
                <input class="form-control input" id="inputtext-2" disabled="disabled" value="@Html.DisplayFor(model => model.Currency)">
            </div>
            <div class="form-item-valid form-group col-12 col-md-6">
                <label class="input-label"> @Html.DisplayNameFor(model => model.CreatedDate)</label>
                <input class="form-control input" id="inputtext-2" disabled="disabled" value="@Html.DisplayFor(model => model.CreatedDate)">
            </div>
            <div class="form-item-valid form-group col-12 col-md-6">
                <label class="input-label"> @Html.DisplayNameFor(model => model.UpdatedDate)</label>
                <input class="form-control input" id="inputtext-2" disabled="disabled" value="@Html.DisplayFor(model => model.UpdatedDate)">
            </div>
            <div class="form-item-valid form-group col-12 col-md-6">
                <label class="input-label"> @Html.DisplayNameFor(model => model.User)</label>
                <input class="form-control input" id="inputtext-2" disabled="disabled" value="@Html.DisplayFor(model => model.User.FullName)">
            </div>
        </div>
    </div>
    <br />
    <div class="root-space__btn-wrapp col-12">
        <a asp-action="Index" class="roots-btn btn btn-secondary mr-3">Back</a>
        <a asp-action="Edit" asp-route-id="@Model.ID" class="roots-btn btn btn-primary">Edit</a>
    </div>
    <br />
</div>